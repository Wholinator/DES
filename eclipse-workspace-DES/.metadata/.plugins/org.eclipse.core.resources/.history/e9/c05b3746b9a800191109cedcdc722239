
public class BitString {
	private Bit[] bits;
	
	
	//bit array constructor
	public BitString(Bit[] bits) {
		setBits(bits);
	}
	
	public BitString(byte[] bytes) {
		//instantiate the bit array
		setBits(new Bit[8*bytes.length]);
		
		
	}

	public Bit[] getBits() {
		return bits;
	}

	public void setBits(Bit[] bits) {
		this.bits = bits;
	}
	
}
